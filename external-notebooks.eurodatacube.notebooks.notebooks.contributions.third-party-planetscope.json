ipt_true_color = \"\"\"\n//VERSION=3\nfunction setup() {\n  return {\n    input: [{\"bands\": [\"B1\", \"B2\", \"B3\"]}],\n    output: { bands: 3}\n  };\n}\n\nvar f = 3000;\nfunction evaluatePixel(sample) {\n  return [sample.B3, sample.B2, sample.B1].map(s => s / f);\n}\n\"\"\"","key":"fZAVLW4A1A"},{"type":"output","id":"ZmXdgEmz9gdfPuhqgoRre","data":[],"key":"BxGmDtXMZL"}],"key":"UC15nSYvGJ"},{"type":"block","kind":"notebook-code","data":{},"children":[{"type":"code","lang":"python","executable":true,"value":"# NDVI\nevalscript_ndvi = \"\"\"\n//VERSION=3\nfunction setup() {\n  return {\n    input: [{\"bands\": [\"B3\", \"B4\"]}],\n    output: { bands: 3}\n  };\n}\n\nfunction evaluatePixel(sample) {\n  var ndvi = (sample.B4 - sample.B3) / (sample.B4 + sample.B3)\n  return colorBlend(ndvi, [0.0, 0.2, 0.6], [[1,0,0], [1,1,0], [0.1,0.30,0]])\n}\n\"\"\"","key":"voCuMiri2p"},{"type":"output","id":"aWT5ROZj2Xt0fiGoWpDs_","data":[],"key":"QYhncM1ADW"}],"key":"K6jFTycK32"},{"type":"block","kind":"notebook-content","data":{},"children":[{"type":"heading","depth":4,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Access data with SH process API","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"NNyCO8C9wj"}],"identifier":"access-data-with-sh-process-api","label":"Access data with SH process API","html_id":"access-data-with-sh-process-api","implicit":true,"key":"lhqfQsoQkF"}],"key":"o6AX3ZLXIt"},{"type":"block","kind":"notebook-code","data":{},"children":[{"type":"code","lang":"python","executable":true,"value":"# Prepare process API request\nbody = {\n    \"input\": {\n        \"bounds\": {\n            \"bbox\": [minx, miny, maxx, maxy],\n            \"properties\":{\n                \"crs\": \"http://www.opengis.net/def/crs/OGC/1.3/CRS84\"\n            }\n        },\n        \"data\": [\n            {\n                \"type\": \"CUSTOM\",\n                \"dataFilter\": {\n                    \"collectionId\": collection_id,\n                    \"timeRange\": {\n                        \"from\": date_from,\n                        \"to\": date_to\n                    },\n                }\n            }\n        ]\n    },\n    \"evalscript\": \"\",\n    \"output\": {\n        \"height\": 512,\n        \"width\": 512,\n        \"responses\": [\n          {\n            \"identifier\": \"default\",\n            \"format\": {\n                  \"type\": \"image/jpeg\",\n                   \"quality\": 100\n                }\n          }\n        ]\n    }\n}","key":"Ni0wekxrTv"},{"type":"output","id":"JvhtdR68V_y5ZOk6DW3wx","data":[],"key":"gzAPdQ2R3x"}],"key":"gcRL3ozJ1J"},{"type":"block","kind":"notebook-code","data":{},"children":[{"type":"code","lang":"python","executable":true,"value":"def override_evalscript_in_body (body, evalscript):\n    body [\"evalscript\"] = evalscript\n    return body","key":"iWZoTxkJwJ"},{"type":"output","id":"grNDaAf9BH2o3F8RRBn0C","data":[],"key":"IXEaBwf2EK"}],"key":"acAHryl7td"},{"type":"block","kind":"notebook-code","data":{},"children":[{"type":"code","lang":"python","executable":true,"value":"response_true_color = oauth.post(\n    'https://services.sentinel-hub.com/api/v1/process', \n    json=override_evalscript_in_body(body, evalscript_true_color)\n)\nresponse_ndvi = oauth.post(\n    'https://services.sentinel-hub.com/api/v1/process', \n    json=override_evalscript_in_body(body, evalscript_ndvi)\n)\nif response_true_color.status_code == response_ndvi.status_code == 200:\n    print(\"Success\")","key":"BjtMhtVqTq"},{"type":"output","id":"toKYUcg_qvetwTl9bS2ro","data":[{"name":"stdout","output_type":"stream","text":"Success\n"}],"key":"gvgvjNuadx"}],"key":"EYY5irebU8"},{"type":"block","kind":"notebook-code","data":{"jupyter":{"source_hidden":true}},"children":[{"type":"code","lang":"python","executable":true,"value":"response_true_color.json()","key":"O6KyINAFl0"},{"type":"output","id":"s4LVxESo2jlD1WAasqg8j","dat